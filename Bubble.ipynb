   "source": [
    "# Bubble sort"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def bubble(arr):\n",
    "    length= len(arr)\n",
    "    for i in range(length):\n",
    "        for j in range(length-i-1):\n",
    "            if(arr[j]>arr[j+1]):\n",
    "                arr[j],arr[j+1]=arr[j+1],arr[j]\n",
    "                "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[0, 1, 1, 3, 4, 5, 7, 8, 9]\n"
     ]
    }
   ],
   "source": [
    "arr=[3,5,1,7,1,0,4,8,9]\n",
    "bubble(arr)\n",
    "print(arr)"
   ]
  }
